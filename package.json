{
  "name": "play-supported-devices",
  "version": "1.0.0",
  "description": "Fast, searchable cache of Google Play supported devices (auto-updated every 3 h)",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist",
    "data/devices.json"
  ],
  "keywords": [
    "android",
    "google-play",
    "devices",
    "npm-package"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohammednudman/play-supported-devices"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup src --format esm,cjs --dts",
    "dev": "tsup src --watch --format esm,cjs --dts",
    "prepublishOnly": "npm run build",
    "update": "node scripts/update.js"
  },
  "author": "Mohammed Nudman Raza Shaikh",
  "dependencies": {
    "jsdom": "^24",
    "node-fetch": "^3"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "tsup": "^8",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
